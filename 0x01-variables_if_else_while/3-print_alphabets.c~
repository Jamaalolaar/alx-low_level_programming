#include <stdio.h>

/**
 * main - to print the lowercase of the alphabet
 * than the uppercase.
 *
 * Return: Always 0.
 */

int main(void)
{
	char alphabet;
	
	for (alphabet = 'a'; alphabet <= 'z'; alphabet++)
	{
		putchar(alphabet);
	}

	for (alphabet = 'A'; alphabet <= 'Z'; alphabet++)
	{
		putchar(alphabet);
	}

	return (0);
}
