#include "main.h"
#include <stdio.h>
#include <stdlib.h>

/**
 * free_grid.c - frees a 2 dimensional grid previously
 * created by your alloc_grid function.
 * @grid: pointer to be freed.
 * @height: grid height.
 * Return: void
 */

void free_grid(int **grid, int height)
{
	int n = 0;

	while (i < height)
	{
		free(grid[i]);
		i++;
	}
	free(grid);
}
