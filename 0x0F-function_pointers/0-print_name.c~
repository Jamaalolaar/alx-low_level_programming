#include "main.h"
#include <stdio.h>
#include <stdlib.h>
#include <stddef.h>

/**
 * print_name - prints a name
 * @name: name of the person
 * @f: pointer to a function
 * Return: void
 */

void print_name(char *name, void (*f)(char *))
{
	int i = 0;

	while (name[i])
	{
		printf("%s", name);
		i++;
	}
}

