#include "lists.h"
/**
 * get_nodeint_at_index - gets a partiular node in a linked list
 * @head: pointer to the head of the list
 * @index: index of the node to be found
 * Return: pointer to the needed node if positive; NULL if negative
 */
listint_t *get_nodeint_at_index(listint_t *head, unsigned int index)
{
